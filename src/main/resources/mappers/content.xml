<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bosung.todolist.domain.todo.TodoRepository">
	<insert id="save" parameterType="com.bosung.todolist.domain.todo.Todo">
		insert into
			todo_mst
		values(
			0,
			#{todo_content},
			0,
			#{importance_flag},
			now(),
			now()
		)
	</insert>
</mapper>